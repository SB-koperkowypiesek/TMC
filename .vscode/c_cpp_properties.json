{
    "configurations": [
        {
            "name": "STM32 F407",
            "includePath": [
                "${workspaceFolder}/**"
            ],
            "defines": [],
            "intelliSenseMode": "gcc-arm",
            "compilerPath": "/bin/arm-none-eabi-gcc",
            "cStandard": "gnu17",
            "cppStandard": "c++17",
            "compilerArgs": [
                "-mcpu=cortex-m4",
                "-mfpu=fpv4-sp-d16",
                "-mfloat-abi=hard",
                "-mthumb"
            ],
            "compileCommands": "${workspaceFolder}/build/compile_commands.json",
            "configurationProvider": "ms-vscode.cmake-tools"
        }
    ],
    "version": 4
}